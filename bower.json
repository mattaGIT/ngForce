{
  "name": "ngForce1",
  "version": "3.3.0",
  "homepage": "https://github.com/mattaGIT/ngForce.git",
  "authors": [
    "Kevin Poorman <kjp@brightleafsoftware.com>",
    "Matthew McKinley <matt@silvelrinecrm.com>"
  ],
  "description": "Fork of ngForce for use with Lightning,VF, and pure HTML apps. Makes including ngForce1 simpler and more modular. Fixes dependency injection. Uses gulp/bower build tools.",
  "main": [
    "./build/js/ngForce1.js"
  ],
  "keywords": [
    "lightning",
    "salesforce",
    "angular"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "restangular": "~1.5.1",
    "lodash": "~3.10.1",
    "stringview": "*",
    "angular": "~1.4.3",
    "bootstrap": "~3.3.5",
    "safeApply": "https://github.com/mattaGIT/AngularJS-Scope.SafeApply.git#~1.0.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    }
  }
}
